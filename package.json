{
  "name": "2yrs_interviews",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html",
    "prebuild": "shx rm -rf dist/*",
    "build": "parcel build ./src/index.html --no-scope-hoist --public-url ./",
    "predeploy": "shx rm -rf dist && parcel build ./src/index.html --public-url 'https://amcglinchy.github.io/2yrs_interviews/'",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "version": "1.0.0",
  "repository": "https://github.com/amcglinchy/2yrs_interviews.git",
  "author": "Alexandra <asmcglinchy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.10.3"
  },
  "dependencies": {
    "bootstrap": "5.3.2",
    "d3": "^7.8.5",
    "gsap": "^3.12.3",
    "intersection-observer": "^0.12.2",
    "push-dir": "^0.4.1",
    "scrollama": "^3.2.0",
    "scrollmagic": "^2.0.8",
    "scrollmagic-plugin-gsap": "^1.0.4",
    "shx": "^0.3.4"
  }
}
